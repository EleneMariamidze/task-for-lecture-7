form abc import ABC, abstractmethod

class Vehicle (ABC):
    @abstractmethod
    def start_engine (self):
        return 'Vehicle started'

    @abstractmethod
    def stop_engine (self):
        return 'Vehicle stopped'

class Car (Vehicle):
    def __init__ (self, max_speed, current_speed):
    self.max_speed = max_speed
    self.current_speed = current_speed

    def start_engine (self):
         return 'car started'
        
    def stop_engine (self):
         return ' car stopped'
 
class SportCar (Car):
    def __init__ (self, max_speed, current_speed):
    self.max_speed = max_speed
    self.current_speed = current_speed

    def start_engine (self):
        print 'car started'


    def stop_engine (self):
        print ' car stopped'

        if self.current_speed = 0:
           print 'car stopped'
        elif self.max_speed =< 50
           print 'car started'
        else self.max_speed >= 240
           print 'this is the maximum speed for this sportcar'

        

    
